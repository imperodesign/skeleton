{
  "name": "skeleton",
  "version": "1.1.0",
  "description": "Starter skeleton for a web app",
  "main": "app/index.js",
  "scripts": {
    "develop": "npm run js:dev",
    "js:dev": "browserify app/static/js/index.js -t babelify --debug -o app/static/dist/index.js",
    "js:prd": "browserify app/static/js/index.js -t babelify -t uglifyify -o app/static/dist/index.js",
    "start": "node --use_strict app/index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imperodesign/skeleton.git"
  },
  "author": "izolate <yoshtalwar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imperodesign/skeleton/issues"
  },
  "homepage": "https://github.com/imperodesign/skeleton",
  "dependencies": {
    "babelify": "^6.1.3",
    "body-parser": "^1.13.3",
    "browserify": "^11.0.0",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.3.5",
    "dotenv": "^1.2.0",
    "express": "^4.13.1",
    "express-mongo-db": "^2.0.3",
    "express-session": "^1.12.1",
    "gulp": "^3.9.0",
    "gulp-csso": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^0.10.0",
    "gulp-sourdough": "^1.0.2",
    "gulp-uglify": "^1.2.0",
    "jade": "^1.11.0",
    "require-dir": "^0.3.0",
    "rupture": "^0.6.1",
    "sourdough-reset": "^0.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
